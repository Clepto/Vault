    <columns>
      <!-- column-name mode -->
      <column type="gchararray"/>
    </columns>
    <data>
      <row>
        <col id="0" translatable="yes">Standard</col>
      </row>
      <row>
        <col id="0" translatable="yes">Paranoia</col>
      </row>
    </data>
  </object>
  <object class="GtkListStore" id="openfoldersstore">
    <columns>
      <!-- column-name openfolders -->
      <column type="gchararray"/>
    </columns>
  </object>
